{
  "name": "dbc",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build --base=./ && npm run cp:bash && npm run exec:scripts",
    "serve": "vite preview",
    "cp:bash": "cp scripts/* dist/assets/",
    "exec:scripts": "cd dist/assets && bash br.bash && bash gzip.bash && bash svg.bash"
  },
  "dependencies": {
    "@antv/l7": "^2.4.3",
    "@babel/core": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@vitejs/plugin-legacy": "^1.3.4",
    "bignumber.js": "^9.0.1",
    "modern-normalize": "^1.1.0",
    "vue": "^3.0.5",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.6",
    "vue-select": "^3.11.2"
  },
  "devDependencies": {
    "@types/vue-select": "^3.11.1",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.5",
    "less": "^4.1.1",
    "postcss-px-to-viewport": "^1.1.1",
    "pug": "^3.0.2",
    "typescript": "^4.1.3",
    "vite": "^2.3.0",
    "vue-tsc": "^0.0.24"
  }
}
